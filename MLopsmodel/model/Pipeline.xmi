<?xml version="1.0" encoding="UTF-8"?>
<mLopsmodel:Pipeline
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:mLopsmodel="http://www.example.org/mLopsmodel"
    xsi:schemaLocation="http://www.example.org/mLopsmodel mLopsmodel.ecore"
    name="test_pipeline"
    version="1">
  <training
      algorithm="RandomForest">
    <parameter
        key="n_estimators"
        value="100"/>
  </training>
  <dependency
      name="numpy"
      version="1.21.0"/>
  <dependency
      name="pandas"
      version="1.3.3"/>
  <dependency
      name="scikit-learn"
      version="0.24.2"/>
  <dataprocessing
      inputPath="/data/input"
      outputPath="/data/output"/>
  <deployment
      targetPlatform="Docker"
      scalability="2"/>
</mLopsmodel:Pipeline>
